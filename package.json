{
  "name": "setgo",
  "version": "0.0.0-development",
  "license": "GPLV3",
  "repository": {
    "type": "git",
    "url": "https://github.com/TomTomB/setgo.git"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "scripts": {
    "commit": "npx git-cz",
    "ng": "nx",
    "prepare": "husky install",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main && yarn build:version",
    "nx": "nx",
    "start": "ng serve",
    "build": "ng build",
    "build:version": "node tools/scripts/generate-version-file.js",
    "test": "ng test",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps --base=origin/main --head=HEAD",
    "affected:libs": "nx affected:libs --base=origin/main --head=HEAD",
    "affected:build": "nx affected:build --base=origin/main --head=HEAD",
    "affected:build:all": "nx affected:build --all",
    "affected:e2e": "nx affected:e2e --base=origin/main --head=HEAD",
    "affected:test": "nx affected:test --base=origin/main --head=HEAD",
    "affected:lint": "nx affected:lint --base=origin/main --head=HEAD",
    "affected:dep-graph": "nx affected:dep-graph --base=origin/main --head=HEAD",
    "affected": "nx affected --base=origin/main --head=HEAD",
    "format": "nx format:write --base=origin/main --head=HEAD",
    "format:write": "nx format:write --base=origin/main --head=HEAD",
    "format:check": "nx format:check --base=origin/main --head=HEAD",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "dev:ssr": "nx run setgo:serve-ssr",
    "serve:ssr": "node dist/apps/setgo/server/main.js",
    "build:ssr:client": "nx build --with-deps",
    "build:ssr:server": "nx run setgo:server",
    "prerender": "nx run setgo:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.1.0",
