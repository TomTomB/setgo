{
  "name": "setgo",
  "version": "0.0.0-development",
  "license": "GPLV3",
  "repository": {
    "type": "git",
    "url": "https://github.com/TomTomB/setgo.git"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "scripts": {
    "commit": "npx git-cz",
    "ng": "nx",
    "prepare": "husky install",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main && yarn build:version",
    "nx": "nx",
    "start": "ng serve",
    "build": "ng build",
    "build:version": "node tools/scripts/generate-version-file.js",
    "test": "ng test",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps --base=origin/main --head=HEAD",
    "affected:libs": "nx affected:libs --base=origin/main --head=HEAD",
    "affected:build": "nx affected:build --base=origin/main --head=HEAD",
    "affected:build:all": "nx affected:build --all",
    "affected:e2e": "nx affected:e2e --base=origin/main --head=HEAD",
    "affected:test": "nx affected:test --base=origin/main --head=HEAD",
    "affected:lint": "nx affected:lint --base=origin/main --head=HEAD",
    "affected:dep-graph": "nx affected:dep-graph --base=origin/main --head=HEAD",
    "affected": "nx affected --base=origin/main --head=HEAD",
    "format": "nx format:write --base=origin/main --head=HEAD",
    "format:write": "nx format:write --base=origin/main --head=HEAD",
    "format:check": "nx format:check --base=origin/main --head=HEAD",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "dev:ssr": "nx run setgo:serve-ssr",
    "serve:ssr": "node dist/apps/setgo/server/main.js",
    "build:ssr": "nx build --with-deps && nx run setgo:server",
    "prerender": "nx run setgo:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.0.5",
    "@angular/common": "^12.0.5",
    "@angular/compiler": "^12.0.5",
    "@angular/core": "^12.0.5",
    "@angular/fire": "6.1.5",
    "@angular/forms": "^12.0.5",
    "@angular/platform-browser": "^12.0.5",
    "@angular/platform-browser-dynamic": "^12.0.5",
    "@angular/platform-server": "^12.0.5",
    "@angular/router": "^12.0.5",
    "@ngrx/component-store": "^12.0.0",
    "@ngrx/effects": "^12.0.0",
    "@ngrx/entity": "^12.0.0",
    "@ngrx/router-store": "^12.0.0",
    "@ngrx/store": "^12.0.0",
    "@nguniversal/express-engine": "12.0.2",
    "@nrwl/angular": "12.4.0",
    "@sentry/angular": "^6.7.1",
    "@sentry/tracing": "^6.7.1",
    "@tomtomb/ngrx-toolkit": "^2.4.2",
    "express": "^4.17.1",
    "firebase": "8.6.8",
    "firebase-admin": "^9.9.0",
    "firebase-functions": "^3.14.1",
    "rxjs": "~6.6.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.0.5",
    "@angular-eslint/eslint-plugin": "~12.2.0",
    "@angular-eslint/eslint-plugin-template": "~12.2.0",
    "@angular-eslint/template-parser": "~12.2.0",
    "@angular/cli": "^12.0.5",
    "@angular/compiler-cli": "^12.0.5",
    "@angular/language-service": "^12.0.5",
    "@ngrx/schematics": "^12.0.0",
    "@ngrx/store-devtools": "^12.0.0",
    "@nguniversal/builders": "^12.0.2",
    "@nrwl/cli": "12.4.0",
    "@nrwl/cypress": "12.4.0",
    "@nrwl/eslint-plugin-nx": "12.4.0",
    "@nrwl/jest": "12.4.0",
    "@nrwl/linter": "12.4.0",
    "@nrwl/nx-cloud": "12.2.7",
    "@nrwl/tao": "12.4.0",
    "@nrwl/workspace": "12.4.0",
    "@simondotm/nx-firebase": "^0.3.2",
    "@types/express": "^4.17.12",
    "@types/jest": "26.0.23",
    "@types/node": "14.17.4",
    "@typescript-eslint/eslint-plugin": "4.28.0",
    "@typescript-eslint/parser": "4.28.0",
    "cypress": "^7.5.0",
    "dotenv": "10.0.0",
    "eslint": "7.29.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-cypress": "^2.11.3",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
