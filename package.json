    "ng-packagr": "^12.0.0",
    "prettier": "2.3.2",
    "rxjs-marbles": "^7.0.0",
    "ts-jest": "27.0.3",
    "ts-node": "~9.1.1",
    "typescript": "~4.2.4"
  }
}
